<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="113.0" prefWidth="836.0" style="-fx-border-color: #c4c4c4;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.omazonproject.SellerReplyController">
    <children>
        <Label fx:id="username" layoutX="14.0" layoutY="13.0" prefHeight="20.0" prefWidth="118.0" text="Username">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Label fx:id="review" layoutX="15.0" layoutY="33.0" prefHeight="20.0" prefWidth="806.0" text="Good product with fast delivery." />
        <Label layoutX="29.0" layoutY="59.0" text="Seller's reply:">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
      <TextField fx:id="reply" layoutX="29.0" layoutY="79.0" prefHeight="26.0" prefWidth="716.0" />
      <Button layoutX="770.0" layoutY="80.0" mnemonicParsing="false" onAction="#replyButtonPressed" text="Reply" />
    </children>
</AnchorPane>
